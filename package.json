{
  "name": "gitfiddle",
  "version": "1.0.0",
  "description": "Visualize git commits and branches",
  "main": "index.js",
  "engine": {
    "node": "14.x"
  },
  "scripts": {
    "lint": "eslint --fix --ext ./server/*.js ./server/**/*.js",
    "transpile": "babel ./server -d ./dist/server",
    "clean": "rimraf ./dist/server",
    "build-server": "npm-run-all lint clean transpile",
    "build-client": "cd client && npm run build && cd ..",
    "build-prod": "npm-run-all build-server build-client",
    "dev": "NODE_ENV=development nodemon ./dist/server/index.js",
    "start": "node ./dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tanvirmislam/gitfiddle.git"
  },
  "keywords": [
    "git",
    "version",
    "control",
    "visualization",
    "tree",
    "branches",
    "git",
    "tree"
  ],
  "author": "Tanvir",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
